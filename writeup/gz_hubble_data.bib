	Doi = {10.1086/375001},
	Eprint = {arXiv:astro-ph/0303065},
	Journal = {\apjs},
	Keywords = {Galaxies: Evolution, Galaxies: Formation, Galaxies: Structure},
	Month = jul,
	Pages = {1-28},
	Title = {{The Relationship between Stellar Light Distributions of Galaxies and Their Formation Histories}},
