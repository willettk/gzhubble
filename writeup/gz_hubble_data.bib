	Doi = {10.1093/mnrasl/slt136},
	Eprint = {1310.0007},
	Journal = {\mnras},
	Keywords = {galaxies: elliptical and lenticular, cD, galaxies: evolution, galaxies: formation, galaxies: interactions, galaxies: starburst},
	Month = jan,
	Pages = {L41-L45},
	Primaryclass = {astro-ph.CO},
	Title = {{The significant contribution of minor mergers to the cosmic star formation budget}},
	Volume = 437,
	Year = 2014,
